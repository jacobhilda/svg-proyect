@model IEnumerable<app.Models.Svg>
@{
    /**/

    ViewBag.Title = "Home Page";
}

<div class="row">
    @foreach (var item in Model)
    {        <div class="col-md-2">
            <div class="thumbnail">
                <a href="/Home/About?name=@item.Name">
                    <img src="@item.Url" alt="Lights" style="width:100%">
                    <div class="caption">
                        <p>Lorem ipsum...</p>
                    </div>
                </a>
            </div>
        </div>
    }
</div>